<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/beache/settings/wcm/templates/page-content"
        jcr:primaryType="cq:PageContent"
        jcr:title="Event Teaser"
        sling:resourceType="beache/components/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="beache/components/container"
            layout="responsiveGrid">
            <container
                jcr:primaryType="nt:unstructured"
                sling:resourceType="beache/components/container"
                layout="responsiveGrid">
                <title
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="beache/components/title"/>
                <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="beache/components/container"
                    layout="responsiveGrid">
                    <text
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="beache/components/text"
                        text="&lt;p>This will be very special.&lt;/p>&#xd;&#xa;"
                        textIsRich="true"/>
                    <image
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="beache/components/image"
                        altValueFromDAM="true"
                        displayPopupTitle="true"
                        fileReference="/content/dam/beache/concert.jpeg"
                        isDecorative="false"
                        titleValueFromDAM="true"/>
                    <button
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Subscribe"
                        sling:resourceType="beache/components/button"
                        link="/content/beache/au/en/subscribe"/>
                </container>
            </container>
        </root>
    </jcr:content>
</jcr:root>
